{
  "functions": [
    {
      "source": ".",
      "codebase": "default",
      "runtime": "python312",
      "entryPoint": "api",
      "trigger": {}
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "api"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
